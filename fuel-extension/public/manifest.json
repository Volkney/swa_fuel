{
    "manifest_version": 3,
    "name": "Fuel Ticket Extension",
    "version": "1.0.0",
    "description": "A fuel ticket calculator extension",
    "action": {
      "default_popup": "index.html"
    },
    "permissions": [
      "activeTab"
    ],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["src/content.js"]
      }
    ]
  }