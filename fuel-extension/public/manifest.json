{
  "manifest_version": 3,
  "name": "Fuel Ticket Extension",
  "version": "1.0.0",
  "description": "A fuel ticket calculator extension",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "host_permissions": [
   "http://oppssuitemain.swacorp.com",
   "https://oppssuitemain.swacorp.com"
  ],
  "icons": {
    "32": "icon32.png"
  }
}